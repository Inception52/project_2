<h1>Search Results</h1>

<table>
  <thead>
  <tr>
    <th>company</th>
    <th>location</th>
    <th>price</th>
    <th>square_footage</th>
    <th>year</th>
    <th>style</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @shouses.each do |house| %>
    <tr>
      <td><%= house.company_id %></td>
      <td><%= house.location %></td>
      <td><%= house.price %></td>
      <td><%= house.square_footage %></td>
      <td><%= house.year %></td>
      <td><%= house.style%></td>
      <td><%= link_to 'Show', house %></td>

    </tr>
  <% end %>
  </tbody>
</table>


